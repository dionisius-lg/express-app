<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="content-type" content="text/html; charset=UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="robots" content="all,follow">
        <title>Administator</title>

        <link rel="stylesheet" href="/vendor/font-awesome/css/all.min.css">
        <link rel="stylesheet" href="/vendor/icheck-bootstrap/css/icheck-bootstrap.min.css">
        <link rel="stylesheet" href="/vendor/jqvmap/css/jqvmap.min.css">
        <link rel="stylesheet" href="/vendor/adminlte/css/adminlte.min.css">
        <link rel="stylesheet" href="/vendor/toastr/css/toastr.min.css" type="text/css">
        <link rel="stylesheet" href="/vendor/sweetalert2/css/sweetalert2.min.css" type="text/css">

        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

        <script src="/vendor/jquery/jquery.min.js"></script>
        <script src="/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
        <script src="/vendor/jqvmap/jquery.vmap.min.js.min.js"></script>
        <script src="/vendor/adminlte/js/adminlte.js"></script>
        <script src="/vendor/toastr/js/toastr.min.js"></script>
        <script src="/vendor/sweetalert2/js/sweetalert2.min.js"></script>
    </head>

    <body class="sidebar-mini control-sidebar-slide-open layout-fixed layout-footer-fixeds">
        <div class="wrapper">
            <nav class="main-header navbar navbar-expand navbar-white navbar-light">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fa fa-bars"></i></a>
                    </li>
                </ul>
                <ul class="navbar-nav ml-auto">
					<li class="nav-item dropdown">
						<a class="nav-link" data-toggle="dropdown" href="#">
							<i class="fas fa-user mr-1"></i>
							<%- session.user.username %>
						</a>
						<div class="dropdown-menu dropdown-menu-right">
							<a href="/auth/logout" class="dropdown-item">
								<i class="nav-icon fas fa-sign-out-alt mr-1"></i>
								Logout
							</a>
						</div>
					</li>
                    
                </ul>
            </nav>

            <aside class="main-sidebar sidebar-dark-primary elevation-4">
                <a href="#" class="brand-link">
                    <!-- <img src="/img/admin-logo.png" alt="Administator Logo" class="brand-image img-circle elevation-3" style="opacity: .8"> -->
                    <span class="brand-text font-weight-light">Administrator</span>
                </a>
                <div class="sidebar">
                    <nav class="mt-2">
                         <ul class="nav nav-pills nav-sidebar flex-column nav-flat nav-child-indent" data-widget="treeview" role="menu" data-accordion="false">
                             <li class="nav-item">
                                <a href="/" class="nav-link">
									<i class="nav-icon fas fa-tachometer-alt"></i>
                                    <p>Dashboard</p>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="/users" class="nav-link">
									<i class="nav-icon fas fa-users"></i>
                                    <p>Users</p>
                                </a>
                            </li>
                            <li class="nav-item">
                                <a href="/students" class="nav-link">
                                    <i class="nav-icon fas fa-users"></i>
                                    <p>Students</p>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </aside>

            <div class="content-wrapper">
                <div class="content-header">
                    <div class="container-fluid">
                        <div class="row mb-2">
                            <div class="col-md-12">
                                <h1 class="m-0 text-dark"><%= pageTitle %></h1>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="content">
                    <div class="container-fluid">
                        <%- include(template) %>
                    </div>
                </div>
            </div>

            <footer class="main-footer">
                <strong><i class="far fa-copyright"></i> 2022</strong>
            </footer>
        </div>

        <% if (messages.success) { %>
            <script type="text/javascript">
                $(document).ready(() => {
                    toastr.success('<%= messages.success %>')
                })
            </script>
            <!-- <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-md-12">
                        <div class="alert alert-primary alert-dismissible  fade show" role="alert">
                            <%= messages.success %>
                            <button type="button" class="btn-close btn-sm" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>
                    </div>
                </div>
            </div> -->
        <% } %>
        
        <% if (messages.error) { %>
            <script type="text/javascript">
                $(document).ready(() => {
                    toastr.success('<%= messages.error %>')
                });
            </script>
            <!-- <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-md-12">
                        <div class="alert alert-danger alert-dismissible  fade show" role="alert">
                            <%= messages.error %>
                            <button type="button" class="btn-close btn-sm" data-bs-dismiss="alert" aria-label="Close"></button>
                        </div>
                    </div>
                </div>
            </div> -->
        <% } %>
    </body>
</html>
